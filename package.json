{
  "name": "some_like_it_neat",
  "version": "1.0.0",
  "description": "A starter theme for WordPress.",
  "author": "Alex Vasquez",
  "license": "GPL-2.0+",
  "homepage": "https://slin.io",
  "main": "assets/js/app.js",
  "scripts": {
    "webpack": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "hot": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "bundle": "cross-env bundle=1 NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && npx rimraf mix-manifest.json",
    "lint": "npm run lint:styles && npm run lint:scripts && npm run lint:php",
    "lint:styles": "cross-env stylelint './assets/styles/**/*.scss' --syntax scss",
    "lint:scripts": "cross-env eslint './assets/js/**/*.js'",
    "lint:php": "cross-env ./vendor/bin/phpcs .",
    "i18n": "npm run i18n:textdomain && npm run i18n:pot",
    "i18n:textdomain": "npx wpi18n addtextdomain --exclude=vendor,node_modules",
    "i18n:pot": "npx wpi18n makepot",
    "rename": "theme-claim"
  },
  "repository": "digisavvy/some-like-it-neat",
  "bugs": {
    "url": "https://github.com/digisavvy/some-like-it-neat/issues"
  },
  "devDependencies": {
    "bourbon": "^5.1.0",
    "bourbon-neat": "^2.0.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-config-wordpress": "^2.0.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "laravel-mix": "^2.1.14",
    "node-wp-i18n": "^1.2.2",
    "normalize-scss": "^7.0.1",
    "postcss-custom-properties": "^6.3.1",
    "postcss-preset-env": "^5.3.0",
    "purify-css": "^1.2.6",
    "purifycss-webpack": "^0.7.0",
    "rimraf": "^2.6.2",
    "stylelint": "^9.9.0",
    "stylelint-config-wordpress": "^13.1.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "folder-zip-sync": "^1.0.5",
    "fs": "0.0.1-security",
    "zip-folder": "^1.0.0"
  }
}
